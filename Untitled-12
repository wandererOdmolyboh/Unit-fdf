int	creat_matrix(char *line)
{
	t_point **matrix;
	int n;
	int y;

	y = 0;
	n = count_new_line(line);
	matrix = (t_point **)malloc(sizeof(t_point *) * n);

	while(y < 12)
	{
		matrix[y] = (t_point*)malloc(sizeof(t_point *) * 19);
		y++;
	}
	func_parse_stroke(matrix ,line);

	return (0);
}


